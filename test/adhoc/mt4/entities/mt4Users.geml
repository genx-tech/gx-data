entity mt4Users
  -- "Mt 4 Users"

  has
    login : integer code("LOGIN") digits(10) bytes(4) -- "Mt 4 Users Login"
    group : text code("GROUP") fixedLength(16) -- "Mt 4 Users Group"
    enable : integer code("ENABLE") digits(10) bytes(4) -- "Mt 4 Users Enable"
    enableChangePass : integer code("ENABLE_CHANGE_PASS") digits(10) bytes(4) -- "Mt 4 Users Enable Change Pass"
    enableReadonly : integer code("ENABLE_READONLY") digits(10) bytes(4) -- "Mt 4 Users Enable Readonly"
    enableOtp : integer code("ENABLE_OTP") digits(10) bytes(4) default("0") -- "Mt 4 Users Enable Otp"
    passwordPhone : text code("PASSWORD_PHONE") fixedLength(32) -- "Mt 4 Users Password Phone"
    name : text code("NAME") fixedLength(128) -- "Mt 4 Users Name"
    country : text code("COUNTRY") fixedLength(32) -- "Mt 4 Users Country"
    city : text code("CITY") fixedLength(32) -- "Mt 4 Users City"
    state : text code("STATE") fixedLength(32) -- "Mt 4 Users State"
    zipcode : text code("ZIPCODE") fixedLength(16) -- "Mt 4 Users Zipcode"
    address : text code("ADDRESS") fixedLength(128) -- "Mt 4 Users Address"
    leadSource : text code("LEAD_SOURCE") fixedLength(32) -- "Mt 4 Users Lead Source"
    phone : text code("PHONE") fixedLength(32) -- "Mt 4 Users Phone"
    email : text code("EMAIL") fixedLength(48) -- "Mt 4 Users Email"
    comment : text code("COMMENT") fixedLength(64) -- "Mt 4 Users Comment"
    id : text code("ID") fixedLength(32) -- "Mt 4 Users Id"
    status : text code("STATUS") fixedLength(16) -- "Mt 4 Users Status"
    regdate : datetime code("REGDATE") -- "Mt 4 Users Regdate"
    lastdate : datetime code("LASTDATE") -- "Mt 4 Users Lastdate"
    leverage : integer code("LEVERAGE") digits(10) bytes(4) -- "Mt 4 Users Leverage"
    agentAccount : integer code("AGENT_ACCOUNT") digits(10) bytes(4) -- "Mt 4 Users Agent Account"
    timestamp : integer code("TIMESTAMP") digits(10) bytes(4) -- "Mt 4 Users Timestamp"
    balance : number code("BALANCE") bytes(8) totalDigits(22) -- "Mt 4 Users Balance"
    prevmonthbalance : number code("PREVMONTHBALANCE") bytes(8) totalDigits(22) -- "Mt 4 Users Prevmonthbalance"
    prevbalance : number code("PREVBALANCE") bytes(8) totalDigits(22) -- "Mt 4 Users Prevbalance"
    credit : number code("CREDIT") bytes(8) totalDigits(22) -- "Mt 4 Users Credit"
    interestrate : number code("INTERESTRATE") bytes(8) totalDigits(22) -- "Mt 4 Users Interestrate"
    taxes : number code("TAXES") bytes(8) totalDigits(22) -- "Mt 4 Users Taxes"
    sendReports : integer code("SEND_REPORTS") digits(10) bytes(4) -- "Mt 4 Users Send Reports"
    mqid : integer code("MQID") digits(10) bytes(4) unsigned default("0") -- "Mt 4 Users Mqid"
    userColor : integer code("USER_COLOR") digits(10) bytes(4) -- "Mt 4 Users User Color"
    equity : number code("EQUITY") bytes(8) totalDigits(22) default("0") -- "Mt 4 Users Equity"
    margin : number code("MARGIN") bytes(8) totalDigits(22) default("0") -- "Mt 4 Users Margin"
    marginLevel : number code("MARGIN_LEVEL") bytes(8) totalDigits(22) default("0") -- "Mt 4 Users Margin Level"
    marginFree : number code("MARGIN_FREE") bytes(8) totalDigits(22) default("0") -- "Mt 4 Users Margin Free"
    currency : text code("CURRENCY") fixedLength(16) -- "Mt 4 Users Currency"
    apiData : binary code("API_DATA") maxLength(65535) optional -- "Mt 4 Users Api Data"
    modifyTime : datetime code("MODIFY_TIME") -- "Mt 4 Users Modify Time"

  key login

  index
    [timestamp]
